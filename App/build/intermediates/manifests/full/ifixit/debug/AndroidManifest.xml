<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dozuki.ifixit"
    android:versionCode="223"
    android:versionName="3.44.6" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.MANAGE_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.WRITE_SYNC_SETTINGS" />
    <uses-permission android:name="android.permission.READ_SYNC_SETTINGS" />
    <uses-permission android:name="android.permission.READ_SYNC_STATS" /> <!-- Permissions required for GCM -->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" /> <!-- Optional permission for App measurement to run. -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <permission
        android:name="com.dozuki.ifixit.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.dozuki.ifixit.permission.C2D_MESSAGE" />

    <application
        android:name="com.dozuki.ifixit.App"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/Theme.Base" >
        <activity
            android:name="com.dozuki.ifixit.ui.topic_view.TopicActivity"
            android:launchMode="singleTop" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dozuki.ifixit.ui.IntentFilterActivity"
            android:exported="true"
            android:taskAffinity="" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <!-- We need to match Guide and Teardown for upper and lower case and also http and https. -->
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/Guide/"
                    android:scheme="http" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/Guide/"
                    android:scheme="https" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/Teardown/"
                    android:scheme="http" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/Teardown/"
                    android:scheme="https" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/guide/"
                    android:scheme="http" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/guide/"
                    android:scheme="https" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/teardown/"
                    android:scheme="http" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/teardown/"
                    android:scheme="https" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/c/"
                    android:scheme="http" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/c/"
                    android:scheme="https" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/Device/"
                    android:scheme="http" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/Device/"
                    android:scheme="https" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/device/"
                    android:scheme="http" />
                <data
                    android:host="www.ifixit.com"
                    android:pathPrefix="/device/"
                    android:scheme="https" />
            </intent-filter>
        </activity>

        <provider
            android:name="com.dozuki.ifixit.ui.search.SearchSuggestionProvider"
            android:authorities="com.dozuki.ifixit.ui.search.SearchSuggestionProvider" />
        <provider
            android:name="com.dozuki.ifixit.util.api.ApiContentProvider"
            android:authorities="com.dozuki.ifixit.provider"
            android:exported="false"
            android:syncable="true" />

        <meta-data
            android:name="android.app.default_searchable"
            android:value="com.dozuki.ifixit.ui.search.SearchActivity" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="com.dozuki.ifixit.ui.search.SearchActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:hardwareAccelerated="true"
            android:label="@string/search"
            android:launchMode="singleTop" >
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name="com.dozuki.ifixit.ui.guide.view.GuideViewActivity"
            android:launchMode="singleTop" />
        <activity android:name="com.dozuki.ifixit.ui.guide.view.TeardownsActivity" />
        <activity android:name="com.dozuki.ifixit.ui.guide.view.FeaturedGuidesActivity" />
        <!--
 Export the activity so it can be launched via PendingIntent in
             a Notification.
        -->
        <activity
            android:name="com.dozuki.ifixit.ui.guide.view.OfflineGuidesActivity"
            android:exported="true" />
        <activity
            android:name="com.dozuki.ifixit.ui.guide.CommentsActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.dozuki.ifixit.ui.gallery.GalleryActivity" />
        <activity android:name="com.dozuki.ifixit.ui.NumberCheckActivity" />
        <activity
            android:name="com.dozuki.ifixit.ui.guide.create.GuideIntroActivity"
            android:launchMode="singleTop" />
        <activity android:name="com.dozuki.ifixit.ui.guide.create.GuideCreateActivity" />
        <activity android:name="com.dozuki.ifixit.ui.guide.create.StepsActivity" />
        <activity
            android:name="com.dozuki.ifixit.ui.guide.create.StepEditActivity"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity android:name="com.dozuki.ifixit.ui.auth.OpenIDActivity" />
        <activity android:name="com.dozuki.ifixit.ui.topic_view.TopicViewActivity" />
        <activity
            android:name="com.dozuki.ifixit.ui.guide.view.FullImageViewActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.dozuki.ifixit.ui.guide.view.VideoViewActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.dozuki.ifixit.ui.guide.view.EmbedViewActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />

        <service android:name="com.dozuki.ifixit.model.auth.AuthenticatorService" >
            <intent-filter>
                <action android:name="android.accounts.AccountAuthenticator" />
            </intent-filter>

            <meta-data
                android:name="android.accounts.AccountAuthenticator"
                android:resource="@xml/authenticator" />
        </service>
        <service
            android:name="com.dozuki.ifixit.util.api.ApiSyncService"
            android:exported="true"
            android:process=":sync" >
            <intent-filter>
                <action android:name="android.content.SyncAdapter" />
            </intent-filter>

            <meta-data
                android:name="android.content.SyncAdapter"
                android:resource="@xml/api_sync_adapter" />
        </service>

        <activity
            android:name="sdk.insert.io.activities.InsertGateActivity"
            android:launchMode="singleInstance" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="insert-2143c30e" />
            </intent-filter>
            <intent-filter>
                <action android:name="2143c30e" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service
            android:name="com.dozuki.ifixit.MyGcmListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <service
            android:name="com.dozuki.ifixit.MyInstanceIdListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>
        <service
            android:name="com.dozuki.ifixit.RegistrationIntentService"
            android:exported="false" >
        </service>

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="com.insert.tsafrir.demo2" />
            </intent-filter>
        </receiver>

        <meta-data
            android:name="insert_debug_logging"
            android:value="true" />
        <meta-data
            android:name="insert_custom_url"
            android:value="https://device.stage.insert.io" />
        <meta-data
            android:name="insert_custom_socket_url"
            android:value="https://ws.stage.insert.io" />
        <meta-data
            android:name="insert_custom_analytics_url"
            android:value="https://data.stage.insert.io" />

        <activity
            android:name="com.google.android.gms.common.api.GoogleApiActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="sdk.insert.io.activities.InsertVisualActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <activity
            android:name="sdk.insert.io.views.video.JCFullScreenActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="landscape" >
        </activity>

        <receiver
            android:name="com.google.android.gms.measurement.AppMeasurementReceiver"
            android:enabled="true" >
            <intent-filter>
                <action android:name="com.google.android.gms.measurement.UPLOAD" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.google.android.gms.measurement.AppMeasurementService"
            android:enabled="true"
            android:exported="false" />

        <provider
            android:name="com.google.firebase.provider.FirebaseInitProvider"
            android:authorities="com.dozuki.ifixit.firebaseinitprovider"
            android:exported="false"
            android:initOrder="100" />

        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="com.dozuki.ifixit" />
            </intent-filter>
        </receiver>
        <!--
 Internal (not exported) receiver used by the app to start its own exported services
             without risk of being spoofed.
        -->
        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdInternalReceiver"
            android:exported="false" />
        <!--
 FirebaseInstanceIdService performs security checks at runtime,
             no need for explicit permissions despite exported="true"
        -->
        <service
            android:name="com.google.firebase.iid.FirebaseInstanceIdService"
            android:exported="true" >
            <intent-filter android:priority="-500" >
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>